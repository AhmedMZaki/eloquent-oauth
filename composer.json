{
    "name": "adamwathan/eloquent-oauth",
    "description": "Stupid simple OAuth authentication with Laravel and Eloquent",
    "authors": [
    {
        "name": "Adam Wathan",
        "email": "adam.wathan@gmail.com"
    }
    ],
    "license": "MIT",
    "repositories": [
    {
        "type": "vcs",
        "url": "https://github.com/adamwathan/patchwork"
    }
    ],
    "require": {
        "php": ">=5.3.0",
        "illuminate/auth": "4.*",
        "illuminate/auth": "4.*",
        "illuminate/session": "4.*",
        "illuminate/database": "4.*",
        "illuminate/support": "4.*",
        "guzzle/guzzle": "~3.7"
    },
    "require-dev": {
        "mockery/mockery": "0.8.0",
        "antecedent/patchwork": "dev-master"
    },
    "autoload": {
        "psr-0": {
            "AdamWathan\\EloquentOAuth\\": "src/"
        }
    },
    "minimum-stability": "dev"
}
